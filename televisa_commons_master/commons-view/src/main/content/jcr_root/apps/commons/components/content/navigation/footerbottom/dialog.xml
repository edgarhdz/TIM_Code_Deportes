<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    width="1000"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="commons.settings.title">
                <items jcr:primaryType="cq:WidgetCollection">
                    <rightsreserved
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.navcorft01.rightsreserved.description"
                        fieldLabel="commons.navcorft01.rightsreserved.title"
                        name="./rights"
                        xtype="textfield"/>
                    <link
                        jcr:primaryType="cq:Widget"
                        collapsed="false"
                        collapsible="false"
                        title="commons.link.title"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <homePathLink
                                jcr:primaryType="cq:Widget"
                                fieldDescription="commons.link.path.description"
                                fieldLabel="commons.link.path.label"
                                name="./pathLogo"
                                xtype="pathfield"/>
                            <textTitle
                                jcr:primaryType="cq:Widget"
                                fieldDescription="commons.link.title-description"
                                fieldLabel="commons.link.title-label"
                                name="./titleLinkLogo"
                                xtype="textfield"/>
                            <linkType
                                jcr:primaryType="cq:Widget"
                                fieldDescription="commons.link.linktype-description"
                                fieldLabel="commons.link.linktype-label"
                                name="./linkTypeLogo"
                                type="select"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <internal
                                        jcr:primaryType="nt:unstructured"
                                        text="commons.link.internallink-label"
                                        value="internal"/>
                                    <external
                                        jcr:primaryType="nt:unstructured"
                                        text="commons.link.externallink-label"
                                        value="external"/>
                                </options>
                            </linkType>
                        </items>
                    </link>
                </items>
            </tab1>
            <tab3
                jcr:primaryType="cq:Panel"
                title="commons.navcorft01.trademark.title">
                <items jcr:primaryType="cq:WidgetCollection">
                    <text
                        jcr:primaryType="cq:Widget"
                        hideLabel="{Boolean}true"
                        name="./trademark"
                        xtype="richtext">
                        <rtePlugins jcr:primaryType="nt:unstructured">
                            <table
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <format
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <lists
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <justify
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <edit
                                jcr:primaryType="nt:unstructured"
                                features="[paste-wordhtml]"/>
                            <findreplace
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <paraformat
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <subsuperscript
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <misctools
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <links
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <spellcheck
                                jcr:primaryType="nt:unstructured"
                                features="*"
                                invalidStyle="background-color: #ffdddd;"/>
                            <undo
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <image
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                        </rtePlugins>
                    </text>
                </items>
            </tab3>
            <tab2
                jcr:primaryType="cq:Widget"
                path="/apps/commons/components/content/base/linklist/dialog/items/items/linkstab.infinity.json"
                xtype="cqinclude"/>
            <tab_footer
                jcr:primaryType="cq:Panel"
                title="commons.navfooter01.columns.label">
                <items jcr:primaryType="cq:WidgetCollection">
                    <columnscenter
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.navfooter01.columns.description"
                        fieldLabel="commons.navfooter01.columns.label"
                        name="./columnscenter"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <one
                                jcr:primaryType="nt:unstructured"
                                text="commons.columns.one.text"
                                value="1"/>
                            <two
                                jcr:primaryType="nt:unstructured"
                                text="commons.columns.two.text"
                                value="2"/>
                            <three
                                jcr:primaryType="nt:unstructured"
                                text="commons.columns.three.text"
                                value="3"/>
                            <four
                                jcr:primaryType="nt:unstructured"
                                text="commons.columns.four.text"
                                value="4"/>
                        </options>
                    </columnscenter>
                </items>
            </tab_footer>
            <tab5
                jcr:primaryType="cq:Panel"
                title="commons.navcorft01.button.title">
                <items jcr:primaryType="cq:WidgetCollection">
                    <showmessage
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.navcorft01.showmessage.description"
                        fieldLabel="commons.navcorft01.showmessage.label"
                        name="./showmessage"
                        xtype="textfield"/>
                    <showtitle
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.navcorft01.showtitle.description"
                        fieldLabel="commons.navcorft01.showtitle.label"
                        name="./showtitle"
                        xtype="textfield"/>
                    <hidemessage
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.navcorft01.hidemessage.description"
                        fieldLabel="commons.navcorft01.hidemessage.label"
                        name="./hidemessage"
                        xtype="textfield"/>
                    <hidetitle
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.navcorft01.hidetitle.description"
                        fieldLabel="commons.navcorft01.hidetitle.label"
                        name="./hidetitle"
                        xtype="textfield"/>
                </items>
            </tab5>
            <devices
                jcr:primaryType="cq:Panel"
                title="deportes.devices.tabtitle.label">
                <items jcr:primaryType="cq:WidgetCollection">
                    <hideDevices
                        jcr:primaryType="cq:Widget"
                        fieldDescription="deportes.devices.hidedevices.description"
                        fieldLabel="deportes.devices.hidedevices.label"
                        name="./hideDevice"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <item1
                                jcr:primaryType="nt:unstructured"
                                text="deportes.devices.item.do-nothing"
                                value=""/>
                            <item2
                                jcr:primaryType="nt:unstructured"
                                text="deportes.devices.item.hide-tablet-mobile"
                                value="hide-tablet"/>
                            <item3
                                jcr:primaryType="nt:unstructured"
                                text="deportes.devices.item.hide-mobile"
                                value="hide-mobile"/>
                        </options>
                    </hideDevices>
                    <resType1
                        jcr:primaryType="cq:Widget"
                        ignoreData="{Boolean}true"
                        name="./image1/sling:resourceType"
                        value="foundation/components/image"
                        xtype="hidden"/>
                    <resType2
                        jcr:primaryType="cq:Widget"
                        ignoreData="{Boolean}true"
                        name="./image2/sling:resourceType"
                        value="foundation/components/image"
                        xtype="hidden"/>
                </items>
            </devices>
        </items>
    </items>
</jcr:root>
