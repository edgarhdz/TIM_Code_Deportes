<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
	<inheritanceTab
                jcr:primaryType="cq:Widget"
                path="/apps/commons/components/content/base/inheritance/inheritanceTab.infinity.json"
                xtype="cqinclude"/>
        <tab1
            jcr:primaryType="cq:Widget"
            title="commons.galleryHistory.settings.title"
            xtype="panel">
            <items jcr:primaryType="cq:WidgetCollection">
                <path
                    jcr:primaryType="cq:Widget"
                    allowBlank="{Boolean}false"
                    blankText="This field is required..."
                    fieldDescription="commons.galleryHistory.path.description"
                    fieldLabel="commons.galleryHistory.path.label"
                    name="./path"
                    rootPath="/content/televisa"
                    rootTitle="Televisa"
                    xtype="pathfield"/>
                <year
                    jcr:primaryType="cq:Widget"
                    allowBlank="{Boolean}false"
                    allowNegative="false"
                    blankText="commons.prominentteaser.patharticle.blanktext"
                    fieldDescription="commons.galleryHistory.year.description"
                    fieldLabel="commons.galleryHistory.year.label"
                    name="./year"
                    xtype="spinner">
                    <strategy
                        jcr:primaryType="nt:unstructured"
                        incrementValue="{Long}1"
                        maxValue="{Long}2050"
                        minValue="{Long}2000"
                        xtype="number"/>
                </year>
                <target
                    jcr:primaryType="cq:Widget"
                    defaultValue="Select"
                    fieldDescription="commons.link.target-description"
                    fieldLabel="commons.link.target-label"
                    name="./linkTarget"
                    type="select"
                    xtype="selection">
                    <options jcr:primaryType="cq:WidgetCollection">
                        <select
                            jcr:primaryType="nt:unstructured"
                            text="commons.link.select.text"
                            value="select"/>
                        <blank
                            jcr:primaryType="nt:unstructured"
                            text="commons.link.target-option-blank-text"
                            value="_blank"/>
                        <self
                            jcr:primaryType="nt:unstructured"
                            text="commons.link.target-option-self-text"
                            value="_self"/>
                    </options>
                </target>
                <notes
                    jcr:primaryType="cq:Widget"
                    allowBlank="{Boolean}false"
                    blankText="commons.prominentteaser.patharticle.blanktext"
                    defaultValue=""
                    fieldDescription="commons.galleryHistory.typeOfNote.description"
                    fieldLabel="commons.galleryHistory.typeOfNote.label"
                    name="./typeOfNote"
                    type="select"
                    xtype="selection">
                    <options jcr:primaryType="cq:WidgetCollection">
                        <articles
                            jcr:primaryType="nt:unstructured"
                            text="commons.galleryHistory.showarticles"
                            value="genericNote"/>
                        <videos
                            jcr:primaryType="nt:unstructured"
                            text="commons.galleryHistory.showvideos"
                            value="video"/>
                        <photogalleries
                            jcr:primaryType="nt:unstructured"
                            text="commons.galleryHistory.showphotogalleries"
                            value="gallery"/>
                    </options>
                </notes>
            </items>
        </tab1>
        <tab2
            jcr:primaryType="cq:Panel"
            title="commons.galleryHistory.titleLink.title">
            <items jcr:primaryType="cq:WidgetCollection">
                <text
                    jcr:primaryType="cq:Widget"
                    fieldDescription="commons.link.description"
                    fieldLabel="commons.link.title"
                    name="./titleLinkText"
                    xtype="textfield"/>
                <linktype
                    jcr:primaryType="cq:Widget"
                    fieldLabel="commons.link.linktype-label"
                    name="./titleLinkType"
                    type="radio"
                    xtype="selection">
                    <options jcr:primaryType="cq:WidgetCollection">
                        <internal
                            jcr:primaryType="nt:unstructured"
                            text="commons.link.internallink-label"
                            value="internal"/>
                        <external
                            jcr:primaryType="nt:unstructured"
                            text="commons.link.externallink-label"
                            value="external"/>
                    </options>
                    <listeners
                        jcr:primaryType="nt:unstructured"
                        loadcontent="function(field, record, path) {  linkLoadContent(field, record, path);}"
                        selectionchanged="function(selection,value, checked){ linkSelectionChanged(selection,value, checked); }"/>
                </linktype>
                <internallink
                    jcr:primaryType="cq:Widget"
                    fieldDescription="commons.link.internallink-description"
                    fieldLabel="commons.link.internallink-label"
                    name="./titleInternalLink"
                    rootPath="/content/televisa"
                    rootTitle="Televisa"
                    xtype="pathfield"/>
                <externallink
                    jcr:primaryType="cq:Widget"
                    fieldDescription="commons.link.externallink-description"
                    fieldLabel="commons.link.externallink-label"
                    name="./titleExternalLink"
                    xtype="textfield"/>
                <target
                    jcr:primaryType="cq:Widget"
                    defaultValue="_self"
                    fieldDescription="commons.link.target-description"
                    fieldLabel="commons.link.target-label"
                    name="./titleLinkTarget"
                    type="select"
                    xtype="selection">
                    <options jcr:primaryType="cq:WidgetCollection">
                        <blank
                            jcr:primaryType="nt:unstructured"
                            text="commons.link.target-option-blank-text"
                            value="_blank"/>
                        <self
                            jcr:primaryType="nt:unstructured"
                            text="commons.link.target-option-self-text"
                            value="_self"/>
                    </options>
                </target>
                <title
                    jcr:primaryType="cq:Widget"
                    fieldDescription="commons.link.title-description"
                    fieldLabel="commons.link.title-label"
                    name="./titleTitle"
                    xtype="textfield"/>
                <hide
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.link.hide-description"
                        fieldLabel="commons.link.hide-label"
                        name="./linkHide"
                        type = "checkbox"
                        xtype="selection"
                        />
            </items>
        </tab2>
        <tab3
            jcr:primaryType="cq:Panel"
            title="commons.galleryHistory.buttons.title">
            <items jcr:primaryType="cq:WidgetCollection">
                <showmessage
                    jcr:primaryType="cq:Widget"
                    fieldDescription="commons.galleryHistory.show.description"
                    fieldLabel="commons.galleryHistory.show.label"
                    name="./show"
                    xtype="textfield"/>
                <showtitle
                    jcr:primaryType="cq:Widget"
                    fieldDescription="commons.galleryHistory.showtitle.description"
                    fieldLabel="commons.galleryHistory.showtitle.label"
                    name="./showtitle"
                    xtype="textfield"/>
                <hidemessage
                    jcr:primaryType="cq:Widget"
                    fieldDescription="commons.galleryHistory.hide.description"
                    fieldLabel="commons.galleryHistory.hide.label"
                    name="./hide"
                    xtype="textfield"/>
                <hidetitle
                    jcr:primaryType="cq:Widget"
                    fieldDescription="commons.galleryHistory.hidetitle.description"
                    fieldLabel="commons.galleryHistory.hidetitle.label"
                    name="./hidetitle"
                    xtype="textfield"/>
            </items>
        </tab3>
        <devices
            jcr:primaryType="cq:Panel"
            title="deportes.devices.tabtitle.label">
            <items jcr:primaryType="cq:WidgetCollection">
                <hideDevices
                    jcr:primaryType="cq:Widget"
                    fieldDescription="deportes.devices.hidedevices.description"
                    fieldLabel="deportes.devices.hidedevices.label"
                    name="./hideDevice"
                    type="select"
                    xtype="selection">
                    <options jcr:primaryType="cq:WidgetCollection">
                        <item1
                            jcr:primaryType="nt:unstructured"
                            text="deportes.devices.item.do-nothing"
                            value=""/>
                        <item2
                            jcr:primaryType="nt:unstructured"
                            text="deportes.devices.item.hide-tablet-mobile"
                            value="hide-tablet"/>
                        <item3
                            jcr:primaryType="nt:unstructured"
                            text="deportes.devices.item.hide-mobile"
                            value="hide-mobile"/>
                    </options>
                </hideDevices>
                <resType1
                    jcr:primaryType="cq:Widget"
                    ignoreData="{Boolean}true"
                    name="./image1/sling:resourceType"
                    value="foundation/components/image"
                    xtype="hidden"/>
                <resType2
                    jcr:primaryType="cq:Widget"
                    ignoreData="{Boolean}true"
                    name="./image2/sling:resourceType"
                    value="foundation/components/image"
                    xtype="hidden"/>
            </items>
        </devices>
    </items>
</jcr:root>
