<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="500"
    width="900"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab0
                jcr:primaryType="cq:Panel"
                title="commons.settings.title">
                <items jcr:primaryType="cq:WidgetCollection">
                    <showHomePageBook
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.navbookmark.description"
                        fieldLabel="commons.navbookmark.label"
                        name="./showHomePageBook"
                        type="checkbox"
                        xtype="selection">
                        <listeners
                            jcr:primaryType="nt:unstructured"
                            render="function(selection) { &#xa;&#x9;if( selection.getValue() == &quot;true&quot; ) {&#xa;&#x9;    selection.findParentByType('panel').getComponent(1).show();&#xa;&#x9;   &#xa;    } else {&#xa;&#x9;    selection.findParentByType('panel').getComponent(1).hide();&#xa;&#x9;    &#xa;    }&#xa;}"
                            selectionchanged="function(selection, value, checked) { &#xa;&#x9;if( checked ) {&#xa;&#x9;    selection.findParentByType('panel').getComponent(1).show();&#xa;        &#xa;&#x9;}else{&#xa;&#x9;    selection.findParentByType('panel').getComponent(1).hide();&#xa;&#x9;    &#xa;&#x9;}&#xa;}"/>
                    </showHomePageBook>
                    <link
                        jcr:primaryType="cq:Widget"
                        collapsed="false"
                        collapsible="false"
                        title="commons.link.text"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <textLink
                                jcr:primaryType="cq:Widget"
                                fieldDescription="commons.link.description"
                                fieldLabel="commons.link.title"
                                name="./text"
                                xtype="textfield"/>
                            <homePathLink
                                jcr:primaryType="cq:Widget"
                                fieldDescription="commons.link.path.description"
                                fieldLabel="commons.link.path.label"
                                name="./path"
                                xtype="textfield"/>
                            <textTitle
                                jcr:primaryType="cq:Widget"
                                fieldDescription="commons.link.title-description"
                                fieldLabel="commons.link.title-label"
                                name="./titleLink"
                                xtype="textfield"/>
                            <linkType
                                jcr:primaryType="cq:Widget"
                                fieldDescription="commons.link.linktype-description"
                                fieldLabel="commons.link.linktype-label"
                                name="./linkType"
                                type="select"
                                xtype="selection">
                                <options jcr:primaryType="cq:WidgetCollection">
                                    <internal
                                        jcr:primaryType="nt:unstructured"
                                        text="commons.link.internallink-label"
                                        value="internal"/>
                                    <external
                                        jcr:primaryType="nt:unstructured"
                                        text="commons.link.externallink-label"
                                        value="external"/>
                                </options>
                            </linkType>
                        </items>
                    </link>
                </items>
            </tab0>
            <tab_footer
                jcr:primaryType="cq:Panel"
                title="commons.navfooter01.columns.label">
                <items jcr:primaryType="cq:WidgetCollection">
                    <columns
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.navfooter01.columns.description"
                        fieldLabel="commons.navfooter01.columns.label"
                        name="./columnsitem"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <one
                                jcr:primaryType="nt:unstructured"
                                text="commons.columns.one.text"
                                value="1"/>
                            <two
                                jcr:primaryType="nt:unstructured"
                                text="commons.columns.two.text"
                                value="2"/>
                            <three
                                jcr:primaryType="nt:unstructured"
                                text="commons.columns.three.text"
                                value="3"/>
                        </options>
                    </columns>
                    <confpage
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.confpage.description"
                        fieldLabel="commons.confpage.label"
                        name="./globalpage"
                        xtype="pathfield"/>
                    <gobackmessage
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.navfooter01.gobacktext.description"
                        fieldLabel="commons.navfooter01.gobacktext.label"
                        name="./gobackmessage"
                        xtype="textfield"/>
                </items>
            </tab_footer>
            <devices
                jcr:primaryType="cq:Panel"
                title="deportes.devices.tabtitle.label">
                <items jcr:primaryType="cq:WidgetCollection">
                    <hideDevices
                        jcr:primaryType="cq:Widget"
                        fieldDescription="deportes.devices.hidedevices.description"
                        fieldLabel="deportes.devices.hidedevices.label"
                        name="./hideDevice"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <item1
                                jcr:primaryType="nt:unstructured"
                                text="deportes.devices.item.do-nothing"
                                value=""/>
                            <item2
                                jcr:primaryType="nt:unstructured"
                                text="deportes.devices.item.hide-tablet-mobile"
                                value="hide-tablet"/>
                            <item3
                                jcr:primaryType="nt:unstructured"
                                text="deportes.devices.item.hide-mobile"
                                value="hide-mobile"/>
                        </options>
                    </hideDevices>
                    <resType1
                        jcr:primaryType="cq:Widget"
                        ignoreData="{Boolean}true"
                        name="./image1/sling:resourceType"
                        value="foundation/components/image"
                        xtype="hidden"/>
                    <resType2
                        jcr:primaryType="cq:Widget"
                        ignoreData="{Boolean}true"
                        name="./image2/sling:resourceType"
                        value="foundation/components/image"
                        xtype="hidden"/>
                </items>
            </devices>
        </items>
    </items>
</jcr:root>
