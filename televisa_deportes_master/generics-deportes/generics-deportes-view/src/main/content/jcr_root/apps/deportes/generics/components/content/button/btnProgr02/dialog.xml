<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
        jcr:primaryType="cq:Dialog"
          height="500"
          title="genericsdeportes.btnProgr02.title"
          width="900"
          xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
	    <inheritanceTab
                jcr:primaryType="cq:Widget"
                path="/apps/commons/components/content/base/inheritance/inheritanceTab.infinity.json"
                xtype="cqinclude"/>
            <settings
                    jcr:primaryType="cq:Panel"
                    title="genericsdeportes.settings.title">
                <items jcr:primaryType="cq:WidgetCollection">
                    <text
                            jcr:primaryType="cq:Widget"
                            fieldDescription="genericsdeportes.btnProgr02.text.description"
                            fieldLabel="genericsdeportes.btnProgr02.label.text"
                            allowBlank="{Boolean}false"
                            name="./text"
                            xtype="textfield"/>
                    <linktype
                            jcr:primaryType="cq:Widget"
                            fieldLabel="genericsdeportes.link.linktype-label"
                            name="./linkType"
                            type="radio"
                            xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <internal
                                    jcr:primaryType="nt:unstructured"
                                    text="genericsdeportes.link.internallink-label"
                                    value="internal"/>
                            <external
                                    jcr:primaryType="nt:unstructured"
                                    text="genericsdeportes.link.externallink-label"
                                    value="external"/>
                        </options>
                        <listeners
                                jcr:primaryType="nt:unstructured"
                                loadcontent="function(field, record, path) {  linkLoadContent(field, record, path);}"
                                selectionchanged="function(selection,value, checked){ linkSelectionChanged(selection,value, checked); }"/>
                    </linktype>
                    <internallink
                            jcr:primaryType="cq:Widget"
                            fieldDescription="genericsdeportes.link.internallink-description"
                            fieldLabel="genericsdeportes.link.internallink-label"
                            name="./internalLink"
                            rootPath="/content/televisa"
                            rootTitle="Televisa"
                            xtype="pathfield"/>
                    <externallink
                            jcr:primaryType="cq:Widget"
                            fieldDescription="genericsdeportes.link.externallink-description"
                            fieldLabel="genericsdeportes.link.externallink-label"
                            name="./externalLink"
                            xtype="textfield"/>
                    <linkTitle
                            jcr:primaryType="cq:Widget"
                            fieldDescription="genericsdeportes.link.title-description"
                            fieldLabel="genericsdeportes.link.title-label"
                            defaultValue=""
                            name="./linkTitle"
                            xtype="textfield"/>
                    <target
                            jcr:primaryType="cq:Widget"
                            defaultValue="_self"
                            fieldDescription="genericsdeportes.link.target-description"
                            fieldLabel="genericsdeportes.link.target-label"
                            name="./linkTarget"
                            type="select"
                            xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <blank
                                    jcr:primaryType="nt:unstructured"
                                    text="genericsdeportes.link.target-option-blank-text"
                                    value="_blank"/>
                            <self
                                    jcr:primaryType="nt:unstructured"
                                    text="genericsdeportes.link.target-option-self-text"
                                    value="_self"/>
                        </options>
                    </target>
                </items>
            </settings>
            <image
                    jcr:primaryType="cq:Panel"
                    title="genericsdeportes.btnProgr02.image.title">
                <items jcr:primaryType="cq:WidgetCollection">
                    <image
                            jcr:primaryType="cq:Widget"
                            allowBlank="{Boolean}false"
                            fieldLabel="genericsdeportes.btnProgr02.image.label"
                            ddGroups="[media]"
                            fileNameParameter="./image/fileName"
                            fileReferenceParameter="./image/fileReference"
                            height="{Long}200"
                            name="./image/file"
                            requestSuffix="/image.img.png"
                            allowUpload="false"
                            xtype="html5smartimage"/>
                    <resType
                            jcr:primaryType="cq:Widget"
                            ignoreData="{Boolean}true"
                            name="./image/sling:resourceType"
                            value="foundation/components/image"
                            xtype="hidden"/>
                </items>
            </image>
            <imageIcon
                    jcr:primaryType="cq:Panel"
                    title="genericsdeportes.btnProgr02.imageIcon.title">
                <items jcr:primaryType="cq:WidgetCollection">
                    <imageButtonHover
                            jcr:primaryType="cq:Widget"
                            fieldLabel="genericsdeportes.btnProgr02.imageIcon.label"
                            ddGroups="[media]"
                            fileNameParameter="./image2/fileName"
                            fileReferenceParameter="./image2/fileReference"
                            height="{Long}200"
                            name="./image2/file"
                            requestSuffix="/image2.img.png"
                            sizeLimit="100"
                            allowUpload="false"
                            xtype="html5smartimage"/>
                    <resType
                            jcr:primaryType="cq:Widget"
                            ignoreData="{Boolean}true"
                            name="./image2/sling:resourceType"
                            value="foundation/components/image"
                            xtype="hidden"/>
                </items>
            </imageIcon>
            <devices
                    jcr:primaryType="cq:Panel"
                    title="deportes.devices.tabtitle.label">
                <items jcr:primaryType="cq:WidgetCollection">
                    <hideDevices
                            jcr:primaryType="cq:Widget"
                            fieldDescription="deportes.devices.hidedevices.description"
                            fieldLabel="deportes.devices.hidedevices.label"
                            defaultValue=""
                            name="./hideDevice"
                            type="select"
                            xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <item1
                                    jcr:primaryType="nt:unstructured"
                                    text="deportes.devices.item.do-nothing"
                                    value=""/>
                            <item2
                                    jcr:primaryType="nt:unstructured"
                                    text="deportes.devices.item.hide-tablet-mobile"
                                    value="hide-tablet"/>
                            <item3
                                    jcr:primaryType="nt:unstructured"
                                    text="deportes.devices.item.hide-mobile"
                                    value="hide-mobile" />
                        </options>
                    </hideDevices>
                </items>
            </devices>
        </items>
</jcr:root>
