<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="commons.twitter.title"
    xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
        <containers
            jcr:primaryType="cq:Panel"
            title="commons.twitterAccount.settings.title">
            <items jcr:primaryType="cq:WidgetCollection">
                <twitterList
                    jcr:primaryType="cq:Widget"
                    fieldDescription="commons.twitterAccount.userids.description"
                    fieldLabel="commons.twitterAccount.id"
                    name="./twitterAccounts"
                    xtype="multifield">
                    <fieldConfig
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        blankText="commons.standoutteaser.patharticle.blanktext"
                        xtype="textfield"/>
                </twitterList>
            </items>
        </containers>
    </items>
</jcr:root>
