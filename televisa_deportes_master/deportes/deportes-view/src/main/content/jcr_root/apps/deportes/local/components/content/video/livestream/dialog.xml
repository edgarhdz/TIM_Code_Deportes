<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="750"
    title="deportes.akamai.title"
    width="1000"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <settings1
                jcr:primaryType="cq:Panel"
                title="commons.video.settings1.title">
                <items jcr:primaryType="cq:WidgetCollection">
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.video.title.description"
                        fieldLabel="commons.video.title.label"
                        id="title"
                        name="./title"
                        xtype="textfield"/>
                    <program
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.video.program.description"
                        fieldLabel="commons.video.program.label"
                        id="program"
                        name="./program"
                        xtype="textfield"/>
                    <url
                        jcr:primaryType="cq:Widget"
                        fieldLabel="URL"
                        name="./url"
                        xtype="textfield"/>
                    <m3u8
                        jcr:primaryType="cq:Widget"
                        fieldLabel="m3u8"
                        name="./m3u8"
                        xtype="textfield"/>
                    <summary
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.video.summary.description"
                        fieldLabel="commons.video.summary.label"
                        id="summary"
                        name="./summary"
                        xtype="textarea"/>
                    <releaseDate
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.video.releaseDate.description"
                        fieldLabel="commons.video.releaseDate.label"
                        id="releaseDate"
                        name="./releaseDate"
                        xtype="datetime"/>
                    <duration
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Duration"
                        name="./duration"
                        xtype="textfield"/>
                    <genre
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Genre"
                        name="./genre"
                        xtype="textfield"/>
                    <language
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Language"
                        name="./language"
                        xtype="textfield"/>
                    <topic
                        jcr:primaryType="cq:Widget"
                        cls="cq-propsdialog-tags"
                        fieldSubLabel="Tags/Keywords"
                        name="./topic"
                        xtype="tags"/>
                </items>
            </settings1>
            <settings2
                jcr:primaryType="cq:Panel"
                title="commons.video.settings2.title">
                <items jcr:primaryType="cq:WidgetCollection">
                    <geoFilter
                        jcr:primaryType="cq:Widget"
                        fieldLabel="commons.video.geoFilter.label"
                        id="geoFilter"
                        name="./geoFilter"
                        type="select"
                        xtype="selection">
                        <listeners
                            jcr:primaryType="nt:unstructured"
                            beforeloadcontent="function(field, record, path){ geoFilterBeforeLoadContent(field,record,path);}"/>
                    </geoFilter>
                    <extras
                        jcr:primaryType="cq:Widget"
                        fieldLabel="commons.video.extras.label"
                        name="./extras"
                        xtype="textfield"/>
                    <season
                        jcr:primaryType="cq:Widget"
                        fieldLabel="commons.video.season.label"
                        id="season"
                        name="./season"
                        xtype="textfield"/>
                    <chapter
                        jcr:primaryType="cq:Widget"
                        fieldLabel="commons.video.chapter.label"
                        id="chapter"
                        name="./chapter"
                        xtype="textfield"/>
                    <source
                        jcr:primaryType="cq:Widget"
                        fieldLabel="commons.video.source.label"
                        name="./source"
                        xtype="textfield"/>
                    <cast
                        jcr:primaryType="cq:Widget"
                        fieldLabel="commons.video.cast.label"
                        name="./cast"
                        namespaces="[elenco]"
                        xtype="tags"/>
                    <videoType
                        jcr:primaryType="cq:Widget"
                        fieldLabel="commons.video.videoType.label"
                        name="./videoType"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <clip
                                jcr:primaryType="nt:unstructured"
                                text="commons.video.clip.text"
                                value="clip"/>
                            <fullEpisode
                                jcr:primaryType="nt:unstructured"
                                text="commons.video.fullEpisode.text"
                                value="full_episode"/>
                        </options>
                    </videoType>
                    <tooltip
                        jcr:primaryType="cq:Widget"
                        fieldLabel="commons.video.tooltip.label"
                        id="tooltip"
                        name="./tooltip"
                        xtype="textarea"/>
                    <author
                        jcr:primaryType="cq:Widget"
                        fieldLabel="commons.video.author.label"
                        name="./author"
                        xtype="textfield"/>
                    <importance
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Importance"
                        name="./importance"
                        xtype="textfield"/>
                    <imageAlt
                        jcr:primaryType="cq:Widget"
                        fieldDescription="commons.video.imageAlt.description"
                        fieldLabel="commons.video.imageAlt.label"
                        name="./imageAlt"
                        xtype="textfield"/>
                </items>
            </settings2>
            <liveData
                jcr:primaryType="cq:Panel"
                title="Live Data">
                <items jcr:primaryType="cq:WidgetCollection">
                    <status
                        jcr:primaryType="cq:Widget"
                        defaultValue="off"
                        fieldLabel="Status"
                        name="./status"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <off
                                jcr:primaryType="nt:unstructured"
                                text="off"
                                value="off"/>
                            <prev
                                jcr:primaryType="nt:unstructured"
                                text="pre"
                                value="pre"/>
                            <on
                                jcr:primaryType="nt:unstructured"
                                text="on"
                                value="on"/>
                            <post
                                jcr:primaryType="nt:unstructured"
                                text="post"
                                value="post"/>
                            <error
                                jcr:primaryType="nt:unstructured"
                                text="error"
                                value="error"/>
                        </options>
                    </status>
                    <initialDate
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Initial Date"
                        id="initialDate"
                        name="./initialDate"
                        xtype="datetime"/>
                    <finishDate
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Finish Date"
                        id="finishDate"
                        name="./finishDate"
                        xtype="datetime"/>
                    <off
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Off"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <offUrl
                                jcr:primaryType="cq:Widget"
                                fieldLabel="deportes.multistream.url1.label"
                                name="./offUrl"
                                xtype="textfield"/>
                            <offImg
                                jcr:primaryType="cq:Widget"
                                allowUpload="{Boolean}false"
                                ddGroups="[media]"
                                fieldLabel="commons.photogalleryfull01.imagefile.label"
                                fileNameParameter="./off/fileName"
                                fileReferenceParameter="./off/fileReference"
                                height="{Long}150"
                                name="./off/file"
                                requestSuffix="/off.img.png"
                                width="{Long}150"
                                xtype="html5smartimage"/>
                        </items>
                    </off>
                    <prev
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Prev"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <prevUrl
                                jcr:primaryType="cq:Widget"
                                fieldLabel="deportes.multistream.url1.label"
                                name="./prevUrl"
                                xtype="textfield"/>
                            <prevImg
                                jcr:primaryType="cq:Widget"
                                allowUpload="{Boolean}false"
                                ddGroups="[media]"
                                fieldLabel="commons.photogalleryfull01.imagefile.label"
                                fileNameParameter="./prev/fileName"
                                fileReferenceParameter="./prev/fileReference"
                                height="{Long}150"
                                name="./prev/file"
                                requestSuffix="/prev.img.png"
                                width="{Long}150"
                                xtype="html5smartimage"/>
                        </items>
                    </prev>
                    <post
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Post"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <postUrl
                                jcr:primaryType="cq:Widget"
                                fieldLabel="deportes.multistream.url1.label"
                                name="./postUrl"
                                xtype="textfield"/>
                            <postImg
                                jcr:primaryType="cq:Widget"
                                allowUpload="{Boolean}false"
                                ddGroups="[media]"
                                fieldLabel="commons.photogalleryfull01.imagefile.label"
                                fileNameParameter="./post/fileName"
                                fileReferenceParameter="./post/fileReference"
                                height="{Long}150"
                                name="./post/file"
                                requestSuffix="/post.img.png"
                                width="{Long}150"
                                xtype="html5smartimage"/>
                        </items>
                    </post>
                    <error
                        jcr:primaryType="cq:Widget"
                        collapsed="{Boolean}false"
                        collapsible="{Boolean}true"
                        title="Error"
                        xtype="dialogfieldset">
                        <items jcr:primaryType="cq:WidgetCollection">
                            <errorUrl
                                jcr:primaryType="cq:Widget"
                                fieldLabel="deportes.multistream.url1.label"
                                name="./errorUrl"
                                xtype="textfield"/>
                            <errorImg
                                jcr:primaryType="cq:Widget"
                                allowUpload="{Boolean}false"
                                ddGroups="[media]"
                                fieldLabel="commons.photogalleryfull01.imagefile.label"
                                fileNameParameter="./error/fileName"
                                fileReferenceParameter="./error/fileReference"
                                height="{Long}150"
                                name="./error/file"
                                requestSuffix="/error.img.png"
                                width="{Long}150"
                                xtype="html5smartimage"/>
                        </items>
                    </error>
                </items>
            </liveData>
            <textcontent
                jcr:primaryType="cq:Panel"
                title="deportes.akamai.textcontent">
                <items jcr:primaryType="cq:WidgetCollection">
                    <short
                        jcr:primaryType="cq:Widget"
                        fieldDescription="deportes.akamai.short.description"
                        fieldLabel="deportes.akamai.short.label"
                        name="./shortDescription"
                        xtype="textarea"/>
                    <showCancha
                        jcr:primaryType="cq:Widget"
                        defaultValue="{Boolean}true"
                        fieldDescription="&lt;br>"
                        fieldLabel="Show Sports Field"
                        name="./showCancha"
                        style="padding-left:20px"
                        type="checkbox"
                        xtype="selection"/>
                </items>
            </textcontent>
            <image
                jcr:primaryType="cq:Widget"
                allowUpload="{Boolean}false"
                ddGroups="[media]"
                fileNameParameter="./img/fileName"
                fileReferenceParameter="./img/fileReference"
                name="./img/file"
                requestSuffix="/img.img.png"
                title="commons.image.title"
                xtype="html5smartimage"/>
            <devices
                jcr:primaryType="cq:Panel"
                title="deportes.devices.tabtitle.label">
                <items jcr:primaryType="cq:WidgetCollection">
                    <hideDevices
                        jcr:primaryType="cq:Widget"
                        fieldDescription="deportes.devices.hidedevices.description"
                        fieldLabel="deportes.devices.hidedevices.label"
                        name="./hideDevice"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <item1
                                jcr:primaryType="nt:unstructured"
                                text="deportes.devices.item.do-nothing"
                                value=""/>
                            <item2
                                jcr:primaryType="nt:unstructured"
                                text="deportes.devices.item.hide-tablet-mobile"
                                value="hide-tablet"/>
                            <item3
                                jcr:primaryType="nt:unstructured"
                                text="deportes.devices.item.hide-mobile"
                                value="hide-mobile"/>
                        </options>
                    </hideDevices>
                    <resTypeOff
                        jcr:primaryType="cq:Widget"
                        ignoreData="true"
                        name="./off/sling:resourceType"
                        value="foundation/components/image"
                        xtype="hidden"/>
                    <resTypePrev
                        jcr:primaryType="cq:Widget"
                        ignoreData="true"
                        name="./prev/sling:resourceType"
                        value="foundation/components/image"
                        xtype="hidden"/>
                    <resTypePost
                        jcr:primaryType="cq:Widget"
                        ignoreData="true"
                        name="./post/sling:resourceType"
                        value="foundation/components/image"
                        xtype="hidden"/>
                    <resTypeError
                        jcr:primaryType="cq:Widget"
                        ignoreData="true"
                        name="./error/sling:resourceType"
                        value="foundation/components/image"
                        xtype="hidden"/>
                    <resType
                        jcr:primaryType="cq:Widget"
                        ignoreData="true"
                        name="./img/sling:resourceType"
                        value="foundation/components/image"
                        xtype="hidden"/>
                </items>
            </devices>
        </items>
    </items>
</jcr:root>
